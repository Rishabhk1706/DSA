https://leetcode.com/problems/maximum-subarray/  Kadane's Algo

class Solution {
    public int maxSubArray(int[] nums) {
        int maxi = nums[0];
        int sum=nums[0];
        for(int i=1;i<nums.length;i++){
            sum+=nums[i];
            if(maxi<sum){
                maxi=sum;
            }
            if(sum<0){
                sum=0;
            }
        }
        return maxi;
    }
}