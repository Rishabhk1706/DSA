https://leetcode.com/problems/find-missing-and-repeated-values/description/ (2965) 01-08-25

class Solution {
    public int[] findMissingAndRepeatedValues(int[][] grid) {
        int n=grid.length;
        int[] freq = new int[(n*n)+1];
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                freq[grid[i][j]]+=1;
            }
        }
        int[] ans = new int[2];
        for(int i=1;i<freq.length;i++){
            if(freq[i]>1){
                ans[0]=i;
            }
            if(freq[i]<1){
                ans[1]=i;
            }
        }
        return ans;
    }
}