https://leetcode.com/problems/longest-increasing-subsequence/description/

//Tabulation
class Solution {
    public int lengthOfLIS(int[] nums) {
        int[] arr = new int[nums.length];
        Arrays.fill(arr,1);
        int maxi=1;
        for(int i=1;i<nums.length;i++){
            for(int j=0;j<i;j++){
                if(nums[j]<nums[i] && arr[j]+1>arr[i]){
                    arr[i] = arr[j]+1;
                }
            }
            maxi = Math.max(maxi,arr[i]);
        }
        return maxi;
    }
}

//Print LIS
class Solution {
    public int lengthOfLIS(int[] nums) {
        int[] arr = new int[nums.length];
        Arrays.fill(arr,1);
        int[] parent = new int[nums.length];
        for(int i=0;i<nums.length;i++){
            parent[i] = i;
        }
        for(int i=1;i<nums.length;i++){
            for(int j=0;j<i;j++){
                if(nums[j]<nums[i] && arr[j]+1>arr[i]){
                    arr[i] = arr[j]+1;
                    parent[i] = j;
                }
            }
        }
        int maxi=-1;
        int ind=-1;
        for(int i=0;i<arr.length;i++){
            if(maxi<arr[i]){
                maxi=arr[i];
                ind=i;
            }
        }
        int[] ans = new int[maxi];
        int m=maxi-1;
        while(true){
            ans[m--] = nums[ind];
            if(parent[ind]!=ind){
                ind=parent[ind];
            }
            else break;
        }
        for(int i=0;i<maxi;i++){
            System.out.print(ans[i]+" ");
        }
        return maxi;
    }
}

//Another Method
class Solution {
    public int lengthOfLIS(int[] nums) {
        List<Integer> list = new ArrayList<>();
        for(int num : nums){
            int i = Collections.binarySearch(list,num);
            if(i<0) i = -(i+1);
            
            if(i==list.size()){
                list.add(num);
            }
            else{
                list.set(i,num);
            }
        }
        return list.size();
    }
}