https://leetcode.com/problems/climbing-stairs/  DP

class Solution {
    public static int fib(int n, int[] arr){
        if(n==1 || n==2) return n;
        if(arr[n]!=-1) return arr[n];
        arr[n] = fib(n-1,arr) + fib(n-2,arr);
        return arr[n];
    }
    public int climbStairs(int n) {
        //DP
        int[] arr = new int[n+1];
        Arrays.fill(arr,-1);
        return fib(n,arr);
    }
}