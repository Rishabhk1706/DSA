https://leetcode.com/problems/maximum-bags-with-full-capacity-of-rocks/  06-10-25

class Solution {
    public int maximumBags(int[] capacity, int[] rocks, int additionalRocks) {
        int[] m = new int[rocks.length];
        for(int i=0;i<rocks.length;i++){
            m[i] = capacity[i]-rocks[i];
        }
        Arrays.sort(m);
        int c=0;
        for(int i=0;i<rocks.length;i++){
            if(additionalRocks>=m[i]){
                c++;
                additionalRocks-=m[i];
            }
            else{
                break;
            }
        }
        return c;
    }
}