https://leetcode.com/problems/plus-one/description 01-01-26

class Solution {
    public int[] plusOne(int[] digits) {
        for(int i=digits.length-1;i>=0;i--){
            digits[i]+=1;
            digits[i]%=10;
            if(digits[i]!=0){
                return digits;
            }
        }
        int[] res = new int[digits.length+1];
        res[0]=1;
        for(int i=0;i<digits.length;i++){
            res[i+1]=digits[i];
        }
        return res;
    }
}