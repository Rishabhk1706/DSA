https://leetcode.com/problems/minimum-operations-to-transform-string/  08-09-25

class Solution {
    public int minOperations(String s) {
        int ops = 0;
        for(char c : s.toCharArray()){
            int d = (26 - (c - 'a')) % 26;
            ops = Math.max(ops,d);
        }
        return ops;
    }
}