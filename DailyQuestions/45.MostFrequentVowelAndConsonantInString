https://leetcode.com/problems/find-most-frequent-vowel-and-consonant

class Solution {
    public int maxFreqSum(String s) {
        int[] v = new int[5];
        int[] c = new int[26];
        for(char ch : s.toCharArray()){
            if(ch=='a'){
                v[0]++;
            }
            else if(ch=='e'){
                v[1]++;
            }
            else if(ch=='i'){
                v[2]++;
            }
            else if(ch=='o'){
                v[3]++;
            }
            else if(ch=='u'){
                v[4]++;
            }
            else{
                c[ch-'a']++;
            }
        }
        int vowels = 0;
        for(int i=0;i<5;i++){
            if(v[i]>vowels) vowels=v[i];
        }
        int consonants = 0;
        for(int i=0;i<26;i++){
            if(c[i]>consonants) consonants=c[i];
        }
        return vowels + consonants;
    }
}