https://leetcode.com/problems/keep-multiplying-found-values-by-two 19-11-25

class Solution {
    public int findFinalValue(int[] nums, int original) {
        while(true){
            boolean found=false;
            for(int i:nums){
                if(original==i){
                    original*=2;
                    found=true;
                    break;
                }
            }
            if(!found) break;
        }
        return original;
    }
}