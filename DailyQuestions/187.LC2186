https://leetcode.com/problems/minimum-number-of-steps-to-make-two-strings-anagram-ii 04-02-26

class Solution {
    public int minSteps(String s, String t) {
        int[] f = new int[26];
        for(char ch : s.toCharArray()){
            f[ch-'a']++;
        }
        for(char ch : t.toCharArray()){
            f[ch-'a']--;
        }
        int c=0;
        for(int i=0;i<26;i++){
            c+=Math.abs(f[i]);
        }
        return c;
    }
}