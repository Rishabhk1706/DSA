https://leetcode.com/problems/power-of-four/ (342) 15-08-25

class Solution {
    public boolean isPowerOfFour(int n) {
        if(n<=0){
            return false;
        }
        while(n%4==0){
            n=n/4;
        }
        if(n==1){
            return true;
        }
        return false;
    }
}