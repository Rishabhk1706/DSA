https://leetcode.com/problems/delete-nodes-from-linked-list-present-in-array 01-11-25

class Solution {
    public ListNode modifiedList(int[] nums, ListNode head) {
        int max=-1;
        for(int i:nums){
            max= (max<i) ? i:max;
        }
        int[] arr=new int[max+1];
        for(int i:nums){
            arr[i]=1;
        }
        ListNode n = new ListNode(0);
        ListNode cur = n;
        while(head!=null){
            if(head.val>=(max+1) || arr[head.val]==0){
                cur.next=head;
                cur=cur.next;
            }
            head=head.next;
        }
        cur.next=null;
        return n.next;
    }
}