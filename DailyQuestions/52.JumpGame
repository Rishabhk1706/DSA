https://leetcode.com/problems/jump-game/   20-09-25

class Solution {
    public boolean canJump(int[] nums) {
        int n = nums.length;
        int maxi=0;
        for(int i=0;i<n;i++){
            if(i>maxi){
                return false;
            }
            maxi = Math.max(maxi, i+nums[i]);
            if(maxi>=n-1){
                return true;
            }
        }
        return true;
    }
}