https://leetcode.com/problems/largest-3-same-digit-number-in-string/description (2264) 14-08-25

class Solution {
    public String largestGoodInteger(String num) {
        int n = num.length();
        String ans = "";
        for(int i=0;i<n-2;i++) {
            if(num.charAt(i)==num.charAt(i+1) && num.charAt(i)==num.charAt(i+2)) {
                if(ans.compareTo(num.substring(i,i+3)) < 0){
                    ans=num.substring(i,i+3);
                }
            }
        }
        return ans;
    }
}