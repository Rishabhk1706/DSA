https://leetcode.com/problems/remove-nth-node-from-end-of-list/description/ 26-10-25

class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode tail = head;
        int c=0;
        while(tail!=null){
            c++;
            tail=tail.next;
        }
        if(c==n){
            return head.next;
        }
        int ans=c-n;
        ListNode ptr=head;
        for(int i=0;i<ans-1;i++){
            ptr=ptr.next;
        }
        if(ptr!=null && ptr.next!=null){
            ptr.next=ptr.next.next;
        }
        return head;
    }
}