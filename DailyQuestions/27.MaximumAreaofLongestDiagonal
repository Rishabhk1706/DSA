https://leetcode.com/problems/maximum-area-of-longest-diagonal-rectangle/ 26-08-25

class Solution {
    public int areaOfMaxDiagonal(int[][] dimensions) {
        double maxi = 0;
        int area = 0;
        for(int i=0;i<dimensions.length;i++){
            int l = dimensions[i][0];
            int b = dimensions[i][1];
            double diagonal = Math.sqrt(l*l + b*b);
            if(maxi<diagonal){
                maxi=diagonal;
                area=l*b;
            }
            else if(maxi==diagonal){
                area=Math.max(area,l*b);
            }
        }
        return area;
    }
}