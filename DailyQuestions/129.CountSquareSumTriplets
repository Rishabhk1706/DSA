https://leetcode.com/problems/count-square-sum-triples 08-12-25

class Solution {
    public int countTriples(int n) {
        int c=0;
        for(int i=1;i<=n;i++){
            int a2 = i*i;
            for(int j=1;j<=n;j++){
                int b2 = j*j;
                int sum = a2 + b2;
                int val = (int) Math.sqrt(sum);
                if(val<=n && val*val==sum){
                    c++;
                }
            }
        }
        return c;
    }
}