https://leetcode.com/problems/minimum-number-of-swaps-to-make-the-string-balanced 09-01-26

class Solution {
    public int minSwaps(String s) {
        int n = 0;
        for(int i=0;i< s.length();i++){
            char ch = s.charAt(i);
            if(ch == '[') 
                n++;
            else if(n>0) 
                n--;
        }
        return (n+1)/2;
    }
}