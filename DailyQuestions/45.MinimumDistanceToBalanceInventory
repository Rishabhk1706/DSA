https://leetcode.com/problems/minimum-discards-to-balance-inventory/ 13-09-25

class Solution {
    public int minArrivalsToDiscard(int[] arrivals, int w, int m) {
        int[] freq = new int[100001];
        int ans=0;
        int low=0;
        boolean[] vis = new boolean[arrivals.length];
        for(int i=0;i<arrivals.length;i++){
            int item = arrivals[i];
            if(freq[item]<m){
                freq[item]++;
                vis[i]=true;
            }
            else{
                ans++;
            }
            if(i-low+1==w){
                if(vis[low]){
                    freq[arrivals[low]]--;
                }
                low++;
            }
        }
        return ans;
    }
}