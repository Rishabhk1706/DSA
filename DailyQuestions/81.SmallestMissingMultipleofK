https://leetcode.com/problems/smallest-missing-multiple-of-k 20-10-25

class Solution {
    public int missingMultiple(int[] nums, int k) {
        HashSet<Integer> set = new HashSet<>();
        for(int i=0;i<nums.length;i++){
            set.add(nums[i]);
        }
        for(int i=1;i>0;i++){
            if(!set.contains(i*k)){
                return i*k;
            }
        }
        return 0;
    }
}