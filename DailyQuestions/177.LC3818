https://leetcode.com/problems/minimum-prefix-removal-to-make-array-strictly-increasing/ 25-01-26

class Solution {
    public int minimumPrefixLength(int[] nums) {
        int ind=-1;
        for(int i=1;i<nums.length;i++){
            if(nums[i-1]>=nums[i]) ind=i-1;
        }
        return ind+1;
    }
}