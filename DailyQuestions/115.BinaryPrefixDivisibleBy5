https://leetcode.com/problems/binary-prefix-divisible-by-5 24-11-25

class Solution {
    public List<Boolean> prefixesDivBy5(int[] nums) {
        List<Boolean> res = new ArrayList<>();
        int val = 0;
        for(int i=0;i<nums.length;i++){
            val = (val*2 + nums[i])%5;
            if(val==0){
                res.add(true);
            }
            else{
                res.add(false);
            }
        }
        return res;
    }
}